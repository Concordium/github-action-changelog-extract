{
  "name": "concordium-github-action-changelog-extract",
  "version": "1.0.0",
  "description": "GitHub Action for extracting a section from a changelog file corresponding to a given version.",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint .",
    "prepare": "ncc build index.js -o dist --source-map --license licenses.txt",
    "test": "jest",
    "all": "npm run lint && npm run prepare && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Concordium/github-action-changelog-extract.git"
  },
  "keywords": [
    "GitHub",
    "Actions",
    "JavaScript"
  ],
  "author": "Concordium Software",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0"
  },
  "devDependencies": {
    "@vercel/ncc": "^0.36.1",
    "eslint": "^8.45.0",
    "jest": "^29.4.3"
  }
}
